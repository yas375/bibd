<%= f.inputs do %>
  <%= f.input :supplier %>
  <%= f.input :description, :input_html => {:rows => 3} %>
<% end %>

<% 5.times { f.object.line_items.build } if f.object.line_items.empty? %>
<%= f.semantic_fields_for :line_items do |fl| %>
  <%= fl.inputs :class => 'inputs line_items' do %>
    <%= fl.input :product %>
    <%= fl.input :quantity %>
  <% end %>
<% end %>
<%= f.buttons %>
